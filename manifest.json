{
  "name": "JK Navigator",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Use J/K/O to navigate with the most popular websites",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": ["jquery-1.7.2.min.js", "keymaster.min.js", "script.js"],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png",
    "256": "icon256.png"
  }
}

